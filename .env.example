GOOGLE_GENERATIVE_AI_API_KEY=****
OPENAI_API_KEY=****
XAI_API_KEY=****
ANTHROPIC_API_KEY=****

# OpenAI-compatible providers
# OpenRouter configuration:
# OPENROUTER_API_KEY=****        # API key from openrouter.ai
# SITE_URL=https://example.com   # Optional: Your site URL for attribution
# SITE_NAME=MCP Client Chatbot   # Optional: Your app name for attribution

# Define OpenRouter models:
# CUSTOM_PROVIDER_MODELS_openrouter=claude-3-opus:anthropic/claude-3-opus,mixtral:mistralai/mixtral-8x7b-instruct,llama-3-70b:meta/llama-3-70b

# Add other custom providers:
# Format: provider_name:base_url:api_key_env_var,...
# CUSTOM_PROVIDERS=localai:http://localhost:8080/v1:LOCALAI_API_KEY,groq:https://api.groq.com/v1:GROQ_API_KEY

# API keys for custom providers (referenced in CUSTOM_PROVIDERS)
# LOCALAI_API_KEY=your_key_here
# GROQ_API_KEY=your_key_here

# Models for custom providers
# Format: display_name:model_id,...
# CUSTOM_PROVIDER_MODELS_localai=llama3:llama-3-70b-chat,wizard:wizard-13b
# CUSTOM_PROVIDER_MODELS_groq=llama3-70b:llama3-70b-v2,mixtral:mixtral-8x7b-32768
POSTGRES_URL=postgres://postgres:postgres@localhost:5432/chatbot
FILEBASE_URL=file:node_modules/local.db
USE_FILE_SYSTEM_DB=true
AUTH_SECRET="****" # Added by `npx auth`. Read more: https://cli.authjs.dev